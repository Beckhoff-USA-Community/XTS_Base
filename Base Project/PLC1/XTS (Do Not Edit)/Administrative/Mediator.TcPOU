    <Method Name="PartMotorModules48vReady" Id="{98a45699-b0a3-05f3-058d-be5a14452817}" FolderPath="Methods\">
      <Declaration><![CDATA[METHOD PartMotorModules48vReady : BOOL
VAR_INPUT
	PartTcIo		: I_TcIoXtsXpuPart;
END_VAR
VAR
	J					: UDINT;
	nModulesInPart		: UDINT;
END_VAR
]]></Declaration>
      <Implementation>
        <ST><![CDATA[// start with ready, to be overwritten below
PartMotorModules48vReady := TRUE;
// protect against no part provided
IF (PartTcIo = 0) THEN
	// this part can't be ready
	PartMotorModules48vReady := FALSE;
	RETURN;
END_IF
// get number of modules
nModulesInPart:= PartTcIo.GetModuleCount();

// query each module
FOR J := 1 TO nModulesInPart DO
	// check for proper status, or simulation
	IF NOT (PartTcIo.ModuleTcIo(UDINT_TO_UINT(J)).GetIsDcLinkVoltageReady()) AND NOT (fbXtsEnvironment.XpuTcIo(1).GetOperationMode() = OperationMode.Simulation) THEN
		PartMotorModules48vReady := FALSE;
	END_IF 
END_FOR]]></ST>
      </Implementation>
    </Method>
    <Method Name="PartMotorModulesEnabled" Id="{b8e95bc3-b41f-0254-06ff-2216e86cf91e}" FolderPath="Methods\">
      <Declaration><![CDATA[METHOD PartMotorModulesEnabled : BOOL
VAR_INPUT
	PartTcIo		: I_TcIoXtsXpuPart;
END_VAR
VAR
	J					: UDINT;
	DriveStatusStorage	: UINT;
	nModulesInPart		: UDINT;
END_VAR
]]></Declaration>
      <Implementation>
        <ST><![CDATA[// start with ready, to be overwritten below
PartMotorModulesEnabled := TRUE;
// protect against no part provided
IF (PartTcIo = 0) THEN
	// this part can't be ready
	PartMotorModulesEnabled := FALSE;
	RETURN;
END_IF
// get number of modules
nModulesInPart:= PartTcIo.GetModuleCount();

// query each module
FOR J := 1 TO nModulesInPart DO
	// get the status
	DriveStatusStorage:= PartTcIo.ModuleTcIo(UDINT_TO_UINT(J)).GetDriveStatusValue();
	// check for proper status, or simulation
	IF NOT (DriveStatusStorage = 4135) AND NOT (fbXtsEnvironment.XpuTcIo(1).GetOperationMode() = OperationMode.Simulation) THEN
		PartMotorModulesEnabled := FALSE;
	END_IF 
END_FOR]]></ST>
      </Implementation>
    </Method>
