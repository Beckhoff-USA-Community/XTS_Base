    <Property Name="AllMotorModules48vReady" Id="{2499191c-7efc-00ea-283b-811d27cdd608}" FolderPath="Properties\">
      <Declaration><![CDATA[{attribute 'monitoring' := 'call'}
PROPERTY AllMotorModules48vReady : Bool]]></Declaration>
      <Get Name="Get" Id="{f41a50ed-5c36-04fd-1f3c-454c5cf177f1}">
        <Declaration><![CDATA[VAR
	I				: UDINT;
	nParts			: UDINT;
	b48vReady	: BOOL;
	PartTcIo		: I_TcIoXtsXpuPart;
END_VAR]]></Declaration>
        <Implementation>
          <ST><![CDATA[// this is calculation and ADS intensive so the result is memozied once per scan

// only run the sum if the calc bit has not been set for this scan
IF (NOT internalMotorModule48v.calcComplete) THEN
	// Monitor DriveStatus to disallow motion commands before high drive has been achieved.
	IF fbXtsEnvironment.P_IsInitialized THEN
	// Get number of parts. 
		nParts:= fbXtsEnvironment.XpuTcIo(1).GetPartCount();
		IF nParts <> 0 THEN
			// Get number of modules per part and check their state
			b48vReady:= TRUE;
			FOR I := 1 TO nParts DO 
				PartTcIo := fbXtsEnvironment.XpuTcIo(1).PartTcIo(UDINT_TO_UINT(I));
				// and the previous result with the result for this part
				b48vReady := b48vReady AND PartMotorModules48vReady(PartTcIo);
			END_FOR
		END_IF
	END_IF

	// memoize the result
	internalMotorModule48v.calcResult := b48vReady;
	internalMotorModule48v.calcComplete := TRUE;
END_IF;

// return the result
AllMotorModules48vReady:= internalMotorModule48v.calcResult;]]></ST>
        </Implementation>
      </Get>
    </Property>
    <Property Name="AllMotorModulesEnabled" Id="{e4a76f20-75da-0820-2f6d-be0ad375c645}" FolderPath="Properties\">
      <Declaration><![CDATA[{attribute 'monitoring' := 'call'}
PROPERTY AllMotorModulesEnabled : Bool]]></Declaration>
      <Get Name="Get" Id="{f8eb48d2-a530-04d2-2b29-7da0e49d088b}">
        <Declaration><![CDATA[VAR
	I				: UDINT;
	nParts			: UDINT;
	bHighDriveReady	: BOOL;
	PartTcIo		: I_TcIoXtsXpuPart;
END_VAR]]></Declaration>
        <Implementation>
          <ST><![CDATA[// this is calculation and ADS intensive so the result is memozied once per scan

// only run the sum if the calc bit has not been set for this scan
IF (NOT internalMotorModuleEnabled.calcComplete) THEN
	// Monitor DriveStatus to disallow motion commands before high drive has been achieved.
	IF fbXtsEnvironment.P_IsInitialized THEN
	// Get number of parts. 
		nParts:= fbXtsEnvironment.XpuTcIo(1).GetPartCount();
		IF nParts <> 0 THEN
			// Get number of modules per part and check their state
			bHighDriveReady:= TRUE;
			FOR I := 1 TO nParts DO 
				PartTcIo := fbXtsEnvironment.XpuTcIo(1).PartTcIo(UDINT_TO_UINT(I));
				// and the previous result with the result for this part
				bHighDriveReady := bHighDriveReady AND PartMotorModulesEnabled(PartTcIo);
			END_FOR
		END_IF
	END_IF

	// memoize the result
	internalMotorModuleEnabled.calcResult := bHighDriveReady;
	internalMotorModuleEnabled.calcComplete := TRUE;
END_IF;

// return the result
AllMotorModulesEnabled:= internalMotorModuleEnabled.calcResult;]]></ST>
        </Implementation>
      </Get>
